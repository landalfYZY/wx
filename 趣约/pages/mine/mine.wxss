page{
  background-color: #f3f3f3
}
.mine{
  background: #ff6633;
  padding: 0 30rpx 30rpx 30rpx;
}
.e1{
  background-color: #ffffff;
  height: 150rpx;
  border-radius: 10rpx;
  box-shadow: 0 0 10px 0 #e1e1e1
}
.e1 .e1item{
  width: 50%;
  display: inline-block;
  text-align: center;
  padding-top: 30rpx
}
.e1 .e1item image{
  width:50rpx;
  height: 50rpx
}
.e1 .e1item view{
  font-size: 26rpx
}
.e2{
  background: #fff;
  border-radius: 10rpx;
  box-shadow: 0 0 10px 0 #e1e1e1
}
.e2item{
  padding: 30rpx 20rpx;
  border-bottom: 1px solid #e1e1e1
}
.e2item:last-child{
  border-bottom: none
}
.e2item image{
  width: 34rpx;
  height: 34rpx
}
.e2item view{
  margin-left: 30rpx
}