  font-size: 30rpx;
}
.payment-panel .number-panel {
  display: flex;
  justify-content: center;
  flex-flow: wrap;
  position: absolute;
  bottom: 0;
  width: 100%;
  animation: show 0.5s forwards;
}
 .payment-panel .number-panel view{
  width: 33%;
  background: #fff;
  line-height: 120rpx;
  text-align: center;
  font-size: 45rpx;
  border-bottom: 1px #f1f1f1 solid;
  border-right: 1px #f1f1f1 solid
}
.payment-panel .number-panel view:active{
